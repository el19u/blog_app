<%= form_with(model: @user) do |f| %>
    <div class="field">
      <%= f.label("email") %>
      <%= f.text_field(:email) %>
    </div>

    <div class="field">
      <%= f.label("password") %>
      <%= f.text_field(:password) %>
    </div>
  
    <div class="submit">
      <%= f.submit("Submit")%>
    </div>
<% end %>